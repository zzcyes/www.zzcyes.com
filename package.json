{
    "name": "my-blog",
    "version": "1.0.0",
    "description": "Zhong Zichen's personal blog",
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 DEBUG=rspack* rspack serve --config rspack.config.js",
        "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=4096 DEBUG=rspack* rspack build --config rspack.config.js",
        "preview": "rspack preview --config rspack.config.js",
        "build:webpack": "webpack --config build/webpack.config.js",
        "dev:webpack": "webpack serve --mode development",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
